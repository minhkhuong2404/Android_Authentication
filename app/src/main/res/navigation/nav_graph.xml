<?xml version="1.0" encoding="utf-8"?>

<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.authentication.Fragment.Home.Home"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_notification"
        android:name="com.example.authentication.Fragment.Notification.Notification"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_notification" />

    <fragment
        android:id="@+id/navigation_search"
        android:name="com.example.authentication.Fragment.Explorer.Explorer"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_explorer" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.authentication.Fragment.Setting.Setting"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_setting" />

</navigation>